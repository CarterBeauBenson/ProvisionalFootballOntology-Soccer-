    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- IAO:0000115/onPitchAct -->

    <owl:Class rdf:about="IAO:0000115/onPitchAct">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/FutOn_00004"/>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/FutOn_00002 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/FutOn_00002">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/carte/ontologies/2023/6/untitled-ontology-63/FutOn:00001"/>
        <obo:IAO_0000115 xml:lang="en">an intentionalMatchAct that represents any action deliberately performed by a referee during amatch.</obo:IAO_0000115>
        <rdfs:comment xml:lang="en">These actions can include making decisions on rule infractions, starting and stopping play, time-keeping, and any other actions aimed at enforcing the rules of thematch and ensuring fair play.</rdfs:comment>
        <rdfs:label xml:lang="en">refereeAct</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/FutOn_00003 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/FutOn_00003">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/carte/ontologies/2023/6/untitled-ontology-63/FutOn:00001"/>
        <obo:IAO_0000115 xml:lang="en">an intentionalMatchAct that represents any action deliberately performed by a team&apos;s manager or coach during amatch.</obo:IAO_0000115>
        <rdfs:comment xml:lang="en">These actions can include strategic decisions, substitutions, tactical instructions, and any other actions aimed at guiding the team&apos;s play and strategy.</rdfs:comment>
        <rdfs:label xml:lang="en">managerAct</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/FutOn_00004 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/FutOn_00004">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/carte/ontologies/2023/6/untitled-ontology-63/FutOn:00001"/>
        <obo:IAO_0000115 xml:lang="en">an intentionalMatchAct that represents any action performed intentionally by a player during amatch.</obo:IAO_0000115>
        <rdfs:label xml:lang="en">playerAct</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/FutOn_00005 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/FutOn_00005">
        <rdfs:subClassOf rdf:resource="IAO:0000115/onPitchAct"/>
        <obo:IAO_0000115 xml:lang="en">is a playerAct that involves actions taken by a player when their team does not have control of the ball.</obo:IAO_0000115>
        <rdfs:label xml:lang="en">outOfPossessionAct</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/FutOn_00006 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/FutOn_00006">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/FutOn_00005"/>
        <obo:IAO_0000115 xml:lang="en">an outOfPossessionAct, that involves a player positioning themselves close to a specific opposing player with the intent of limiting their influence on thematch by preventing them from receiving the ball or obstructing their plays when they do have the ball,</obo:IAO_0000115>
        <rdfs:label xml:lang="en">markingAct</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/FutOn_00007 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/FutOn_00007">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/FutOn_00006"/>
        <obo:IAO_0000115 xml:lang="en">is a markingAct where a defender is responsible for guarding a specific area of the pitch, rather than a specific player. The defender attempts to intercept any opponent or ball that enters their zone.</obo:IAO_0000115>
        <rdfs:label xml:lang="en">manMarkingAct</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/FutOn_00008 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/FutOn_00008">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/FutOn_00006"/>
        <obo:IAO_0000115 xml:lang="en">a markingAct where a defender is assigned a specific player from the opposing team to guard, with the intent to limit their influence on thematch. The defender typically shadows their assigned player, staying close and attempting to disrupt their play.</obo:IAO_0000115>
        <rdfs:label xml:lang="en">zonalMarkingAct</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/FutOn_00009 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/FutOn_00009">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/FutOn_00005"/>
        <obo:IAO_0000115 xml:lang="en">an outOfPossessionAct that is performed by a player when they attempt to dispossess an opponent by physically challenging them for the ball.</obo:IAO_0000115>
        <rdfs:label xml:lang="en">tackleAct</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/FutOn_00010 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/FutOn_00010">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/FutOn_00009"/>
        <obo:IAO_0000115 xml:lang="en">a tackleAct that is performed by a player when they position themselves in the path of the ball, typically while it is being shot or passed by an opponent, in an attempt to stop its progress or change its direction.</obo:IAO_0000115>
        <rdfs:label xml:lang="en">blockTackleAct</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/FutOn_00011 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/FutOn_00011">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/FutOn_00009"/>
        <obo:IAO_0000115 xml:lang="en">a tackleAct that is performed by a player when they slide on the ground to challenge an opponent for the ball, typically with one leg extended.</obo:IAO_0000115>
        <rdfs:label xml:lang="en">slideTackleAct</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/FutOn_00012 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/FutOn_00012">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/FutOn_00005"/>
        <obo:IAO_0000115 xml:lang="en">an outOfPossessionAct that is performed by a player when they disrupt an opponent&apos;s pass by moving into the ball&apos;s path, thereby gaining possession for their own team.</obo:IAO_0000115>
        <rdfs:label xml:lang="en">interceptionAct</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/FutOn_00013 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/FutOn_00013">
        <rdfs:subClassOf rdf:resource="IAO:0000115/onPitchAct"/>
        <obo:IAO_0000115 xml:lang="en">inPossessionAct is a playerAct that involves actions executed by players of the team that currently has control of the ball.</obo:IAO_0000115>
        <rdfs:label xml:lang="en">inPossessionAct</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/FutOn_00014 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/FutOn_00014">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/FutOn_00112"/>
        <obo:IAO_0000115 xml:lang="en">rotationAct is an offBallAct that involves the strategic movement of a player who does not directly control the ball.</obo:IAO_0000115>
        <rdfs:label xml:lang="en">rotationAct</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/FutOn_00015 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/FutOn_00015">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/FutOn_00014"/>
        <obo:IAO_0000115 xml:lang="en">symmetricalRotationAct in football is a rotationAct that describes a strategic movement or action in which players rotate and change position in a manner that maintains symmetry about an axis.</obo:IAO_0000115>
        <rdfs:label xml:lang="en">symmetricalRotationAct</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/FutOn_00016 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/FutOn_00016">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/FutOn_00014"/>
        <obo:IAO_0000115 xml:lang="en">asymmetricalRotationAct in football is a rotationAct that describes a strategic movement or action in which players rotate and change position in a manner that does not maintain symmetry about an axis.</obo:IAO_0000115>
        <rdfs:label xml:lang="en">asymmetricalRotationAct</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/FutOn_00017 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/FutOn_00017">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/FutOn_00013"/>
        <obo:IAO_0000115 xml:lang="en">onBallAct is an inPossessionAct that involves a player maintaining control over the ball duringmatchplay.</obo:IAO_0000115>
        <rdfs:label xml:lang="en">onBallAct</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/FutOn_00018 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/FutOn_00018">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/FutOn_00017"/>
        <obo:IAO_0000115 xml:lang="en">shotAct is an onBallAct that is performed by a player when they attempt to propel the ball into the opponent&apos;s goal.</obo:IAO_0000115>
        <rdfs:label xml:lang="en">shotAct</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/FutOn_00019 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/FutOn_00019">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/FutOn_00018"/>
        <obo:IAO_0000115 xml:lang="en">bendingShotAct is a shotAct that is performed by a player when they strike the ball in a way that causes it to curve in the air.</obo:IAO_0000115>
        <rdfs:label xml:lang="en">bendingShotAct</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/FutOn_00020 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/FutOn_00020">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/FutOn_00018"/>
        <obo:IAO_0000115 xml:lang="en">bicycleShotAct is a shotAct that is performed by a player when they strike the ball in mid-air with a bicycle kick, a move where the player kicks the ball while performing a backflip.</obo:IAO_0000115>
        <rdfs:label xml:lang="en">bicycleShotAct</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/FutOn_00021 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/FutOn_00021">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/FutOn_00018"/>
        <obo:IAO_0000115 xml:lang="en">chipShotAct is a shotAct that is performed by a player when they strike the ball in an upward trajectory with the aim of lobbing it over the goalkeeper and into the net.</obo:IAO_0000115>
        <rdfs:label xml:lang="en">chipShotAct</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/FutOn_00022 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/FutOn_00022">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/FutOn_00018"/>
        <obo:IAO_0000115 xml:lang="en">headerShotAct is a shotAct that is performed by a player when they attempt to direct the ball into the opponent&apos;s goal by striking it with their head.</obo:IAO_0000115>
        <rdfs:label xml:lang="en">headerShotAct</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/FutOn_00023 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/FutOn_00023">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/FutOn_00018"/>
        <obo:IAO_0000115 xml:lang="en">rabonaShotAct is a shotAct that is performed by a player when they wrap their kicking leg around the back of their standing leg to strike the ball.</obo:IAO_0000115>
        <rdfs:label xml:lang="en">rabonaShotAct</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/FutOn_00024 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/FutOn_00024">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/FutOn_00018"/>
        <obo:IAO_0000115 xml:lang="en">standardShotAct is a shotAct that is performed by a player when they attempt to direct the ball into the opponent&apos;s goal by striking it with a standard kick, typically using the inside of the foot.</obo:IAO_0000115>
        <rdfs:label xml:lang="en">standardShotAct</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/FutOn_00025 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/FutOn_00025">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/FutOn_00018"/>
        <obo:IAO_0000115 xml:lang="en">straightShotAct is a shotAct that is performed by a player when they strike the ball in a straight trajectory towards the opponent&apos;s goal.</obo:IAO_0000115>
        <rdfs:label xml:lang="en">straightShotAct</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/FutOn_00026 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/FutOn_00026">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/FutOn_00018"/>
        <obo:IAO_0000115 xml:lang="en">volleyShotAct is a shotAct that is performed by a player when they strike the ball before it touches the ground, typically after a cross or a rebound.</obo:IAO_0000115>
        <rdfs:label xml:lang="en">volleyShotAct</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/FutOn_00031 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/FutOn_00031">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/FutOn_00015"/>
        <obo:IAO_0000115 xml:lang="en">dribbleAct is an onBallAct that involves a player controlling the ball and moving it forward by repeatedly striking it with any part legal part of their body.</obo:IAO_0000115>
        <rdfs:label xml:lang="en">dribbleAct</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/FutOn_00032 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/FutOn_00032">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/FutOn_00015"/>
        <obo:IAO_0000115 xml:lang="en">controlAct is an onBallAct that represents the action of a player successfully gaining possession of the ball from a pass or interception using any legal part of their body.</obo:IAO_0000115>
        <rdfs:label xml:lang="en">controlAct</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/FutOn_00033 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/FutOn_00033">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/FutOn_00032"/>
        <obo:IAO_0000115 xml:lang="en">chestControl is a controlAct that describes the action of a player successfully gaining possession of the ball using their chest.</obo:IAO_0000115>
        <rdfs:label xml:lang="en">chestControl</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/FutOn_00034 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/FutOn_00034">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/FutOn_00032"/>
        <obo:IAO_0000115 xml:lang="en">footControl is a controlAct that describes the action of a player successfully gaining possession of the ball using their foot.</obo:IAO_0000115>
        <rdfs:label xml:lang="en">footControl</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/FutOn_00035 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/FutOn_00035">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/FutOn_00015"/>
        <obo:IAO_0000115 xml:lang="en">passAct is an onBallAct in which a player, who is in control of the ball, deliberately sends the ball to a teammate.</obo:IAO_0000115>
        <rdfs:label xml:lang="en">passAct</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/FutOn_00036 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/FutOn_00036">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/FutOn_00035"/>
        <obo:IAO_0000115 xml:lang="en">crossAct is passAct that involves sending the ball across the field, usually from a wide position into the penalty area, aiming to reach a teammate.</obo:IAO_0000115>
        <rdfs:label xml:lang="en">crossAct</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/FutOn_00037 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/FutOn_00037">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/FutOn_00035"/>
        <obo:IAO_0000115 xml:lang="en">giveAndGoAct is a passAct that consists of a player passing the ball to a teammate and then immediately moving forward to receive a return pass.</obo:IAO_0000115>
        <rdfs:label xml:lang="en">giveAndGoAct</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/FutOn_00038 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/FutOn_00038">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/FutOn_00035"/>
        <obo:IAO_0000115 xml:lang="en">oneTouchPassAct is a passAct where the player passes the ball immediately upon receiving it, using just one touch.</obo:IAO_0000115>
        <rdfs:label xml:lang="en">oneTouchPassAct</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/FutOn_00039 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/FutOn_00039">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/FutOn_00035"/>
        <obo:IAO_0000115 xml:lang="en">throughPassAct is a passAct in which a player sends the ball through the opponents&apos; defensive line to a teammate, typically to create a scoring opportunity.</obo:IAO_0000115>
        <rdfs:label xml:lang="en">throughPassAct</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/FutOn_00040 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/FutOn_00040">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/FutOn_00035"/>
        <obo:IAO_0000115 xml:lang="en">backHeelPassAct is a passAct that involves the player striking the ball with the back of their heel to pass it to a teammate, often employed to deceive the opposition.</obo:IAO_0000115>
        <rdfs:label xml:lang="en">backHeelPassAct</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/FutOn_00041 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/FutOn_00041">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/FutOn_00035"/>
        <obo:IAO_0000115 xml:lang="en">outsideFootPass is a passAct where the player uses the outside of their foot to pass the ball, typically to curve it around an opponent or for a change in direction.</obo:IAO_0000115>
        <rdfs:label xml:lang="en">outsideFootPass</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/FutOn_00042 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/FutOn_00042">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/FutOn_00035"/>
        <obo:IAO_0000115 xml:lang="en">pushPassAct is a passAct that involves the player using the inside of their foot to gently push the ball to a teammate, typically used for short, accurate passes.</obo:IAO_0000115>
        <rdfs:label xml:lang="en">pushPassAct</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/FutOn_00044 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/FutOn_00044">
        <rdfs:subClassOf rdf:resource="http://www.ontologyrepository.com/CommonCoreOntologies/MeasurementInformationContentEntity"/>
        <obo:IAO_0000115 xml:lang="en">A measurementInformationContentEntity that quantifies or qualifies actions that take place in amatch.</obo:IAO_0000115>
        <rdfs:label xml:lang="en">MatchMeasurement</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/FutOn_00045 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/FutOn_00045">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/FutOn_00044"/>
        <obo:IAO_0000115 xml:lang="en">AmatchMeasurement that represents a numeric or quantifiable property derived from amatch ormatch-related event.</obo:IAO_0000115>
        <rdfs:label xml:lang="en">MatchMeasurementQuantity</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/FutOn_00046 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/FutOn_00046">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/FutOn_00045"/>
        <obo:IAO_0000115 xml:lang="en">AmatchMeasurementQuantity that quantifies the total number of successful tackles performed by a player or a team during amatch.</obo:IAO_0000115>
        <rdfs:label xml:lang="en">tackleTally</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/FutOn_00047 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/FutOn_00047">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/FutOn_00045"/>
        <obo:IAO_0000115 xml:lang="en">AmatchMeasurementQuantity that quantifies the total number of successful interceptions performed by a player or a team during amatch.</obo:IAO_0000115>
        <rdfs:label xml:lang="en">interceptionTally</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/FutOn_00048 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/FutOn_00048">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/FutOn_00045"/>
        <obo:IAO_0000115 xml:lang="en">AmatchMeasurementQuantity that quantifies the total number of successful goals scored by a player or a team during amatch.</obo:IAO_0000115>
        <rdfs:label xml:lang="en">goalTally</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/FutOn_00049 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/FutOn_00049">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/FutOn_00045"/>
        <obo:IAO_0000115 xml:lang="en">AmatchMeasurementQuantity that specifically quantifies the number of shots taken or made during amatch.</obo:IAO_0000115>
        <rdfs:label xml:lang="en">shotTally</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/FutOn_00050 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/FutOn_00050">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/FutOn_00049"/>
        <obo:IAO_0000115 xml:lang="en">A shotTally that quantifies the total number of shots taken or made by an entire team during amatch.</obo:IAO_0000115>
        <rdfs:label xml:lang="en">teamShotTally</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/FutOn_00051 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/FutOn_00051">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/FutOn_00049"/>
        <obo:IAO_0000115 xml:lang="en">A shotTally that quantifies the total number of shots taken or made by an individual player during amatch.</obo:IAO_0000115>
        <rdfs:label xml:lang="en">playerShotTally</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/FutOn_00052 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/FutOn_00052">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/FutOn_00045"/>
        <obo:IAO_0000115 xml:lang="en">AmatchMeasurementQuantity that specifically quantifies the total number of passes made during amatch.</obo:IAO_0000115>
        <rdfs:label xml:lang="en">passTally</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/FutOn_00053 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/FutOn_00053">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/FutOn_00052"/>
        <obo:IAO_0000115 xml:lang="en">A passTally that quantifies the total number of passes made by an individual player during amatch.</obo:IAO_0000115>
        <rdfs:label xml:lang="en">playerPassTally</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/FutOn_00054 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/FutOn_00054">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/FutOn_00052"/>
        <obo:IAO_0000115 xml:lang="en">A passTally that quantifies the total number of passes made by an entire team during amatch ormatch-related event.</obo:IAO_0000115>
        <rdfs:label xml:lang="en">teamPassTally</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/FutOn_00055 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/FutOn_00055">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/FutOn_00045"/>
        <obo:IAO_0000115 xml:lang="en">AmatchMeasurementQuantity that specifically represents the output of an xGAlgorithm, quantifying the probability of a shot resulting in a goal in amatch.</obo:IAO_0000115>
        <rdfs:label xml:lang="en">xGValue</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/FutOn_00056 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/FutOn_00056">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/FutOn_00055"/>
        <obo:IAO_0000115 xml:lang="en">An xGValue that represents the xGValue for an individual player.</obo:IAO_0000115>
        <rdfs:label xml:lang="en">xGValuePlayer</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/FutOn_00057 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/FutOn_00057">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/FutOn_00055"/>
        <obo:IAO_0000115 xml:lang="en">An xGValue that represents the total Expected Goals (xG) value for a team in amatch, calculated as the sum of the xGValues for all individual players on the team.</obo:IAO_0000115>
        <rdfs:label xml:lang="en">xGValueTeam</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/FutOn_00058 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/FutOn_00058">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/FutOn_00045"/>
        <obo:IAO_0000115 xml:lang="en">AmatchMeasurementQuantity that quantifies the likelihood that a given pass will lead to a shot and subsequently a goal, based on various factors such as the type of pass, its origin and destination points, and the historical data.</obo:IAO_0000115>
        <rdfs:label xml:lang="en">xAValue</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/FutOn_00059 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/FutOn_00059">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/FutOn_00045"/>
        <obo:IAO_0000115 xml:lang="en">AmatchMeasurementQuantity that quantifies the predicted number of points a team is expected to earn from amatch based on the team&apos;s total xGValue, which is calculated from individual shot xGValues during thematch.</obo:IAO_0000115>
        <rdfs:label xml:lang="en">xPValue</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/FutOn_00060 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/FutOn_00060">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/FutOn_00044"/>
        <obo:IAO_0000115 xml:lang="en">AmatchMeasurement that specifically represents a qualitative or descriptive attribute associated with amatch.</obo:IAO_0000115>
        <rdfs:label xml:lang="en">MatchMeasurementQuality</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/FutOn_00061 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/FutOn_00061">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/FutOn_00060"/>
        <obo:IAO_0000115 xml:lang="en">AmatchMeasurementQuality that involves a comprehensive evaluation ofmatch dynamics, player performance, and team strategy, based on the xG values and potentially including other qualitative assessments.</obo:IAO_0000115>
        <rdfs:label xml:lang="en">xGAnalysis</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/FutOn_00062 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/FutOn_00062">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/FutOn_00060"/>
        <obo:IAO_0000115 xml:lang="en">AmatchMeasurementQuality that assesses the quality of an individual player&apos;s performance during amatch ormatch-related event, often on a scale (e.g., 1-10).</obo:IAO_0000115>
        <rdfs:label xml:lang="en">playerPerformanceRating</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/FutOn_00063 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/FutOn_00063">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/FutOn_00060"/>
        <obo:IAO_0000115 xml:lang="en">AmatchMeasurementQuality that identifies a specific phase or period of amatch ormatch-related event (e.g., first half, second half, overtime).</obo:IAO_0000115>
        <rdfs:label xml:lang="en">MatchPhaseIdentification</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/FutOn_00064 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/FutOn_00064">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/FutOn_00060"/>
        <obo:IAO_0000115 xml:lang="en">AmatchMeasurementQuality that represents an evaluation of the effectiveness of a player&apos;s markingAct in amatch. This may be quantified by a number of factors, including but not limited to the number of times the marked opponent was dispossessed, the number of unsuccessful plays made by the marked opponent, the marked opponent&apos;s influence on thematch relative to their average performance, and the outcome of thematch.</obo:IAO_0000115>
        <rdfs:label xml:lang="en">markingQuality</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/FutOn_00065 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/FutOn_00065">
        <rdfs:subClassOf rdf:resource="http://www.ontologyrepository.com/CommonCoreOntologies/Algorithm"/>
        <obo:IAO_0000115 xml:lang="en">An Algorithm that is specifically designed to take as input data about a shot, such as its distance, angle, context (e.g., open play, set-piece, etc.), and potentially other factors, and output an xGValue.</obo:IAO_0000115>
        <rdfs:label xml:lang="en">xGAlgorithm</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/FutOn_00066 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/FutOn_00066">
        <rdfs:subClassOf rdf:resource="http://www.ontologyrepository.com/CommonCoreOntologies/Algorithm"/>
        <obo:IAO_0000115 xml:lang="en">An Algorithm that is specifically designed to take as input data about a pass, such as its type, origin and destination points, and other contextual factors, and output an xAValue, which quantifies the likelihood that the given pass will lead to a shot and subsequently a goal.</obo:IAO_0000115>
        <rdfs:label xml:lang="en">xAAlgorithm</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/FutOn_00068 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/FutOn_00068">
        <rdfs:subClassOf rdf:resource="http://www.ontologyrepository.com/CommonCoreOntologies/PredictiveInformationContentEntity"/>
        <obo:IAO_0000115 xml:lang="en">A PredictiveInformationContentEntity used to predict the outcome of a footballmatch or series ofmatches, based on the sum of Expected Goals (xG) values of a team. It predicts the number of points a team is likely to earn from thematch(es).</obo:IAO_0000115>
        <rdfs:label xml:lang="en">xPForGamblers</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/FutOn_00069 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/FutOn_00069">
        <rdfs:subClassOf rdf:resource="http://www.ontologyrepository.com/CommonCoreOntologies/PredictiveInformationContentEntity"/>
        <obo:IAO_0000115 xml:lang="en">A PredictiveInformationContentEntity used to predict the outcome of a footballmatch, based on the quantity and quality of shots taken by a team or player. It quantifies the probability of each shot resulting in a goal, considering various factors like distance from the goal, angle to the goal,match context, and historical data.</obo:IAO_0000115>
        <rdfs:label xml:lang="en">xGforGamblers</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/FutOn_00070 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/FutOn_00070">
        <rdfs:subClassOf rdf:resource="http://www.ontologyrepository.com/CommonCoreOntologies/Ideology"/>
        <obo:IAO_0000115 xml:lang="en">An Ideology that represents the guiding principles, values, and strategic approach a football club adopts and adheres to in its operations, including player development,match strategy, and club management.</obo:IAO_0000115>
        <rdfs:label xml:lang="en">clubPhilosophy</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/FutOn_00071 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/FutOn_00071">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/FutOn_00070"/>
        <obo:IAO_0000115 xml:lang="en">A clubPhilosophy that emphasizes the use of Expected Goals (xG) metrics in various aspects of the football club&apos;s operations and strategy, including player performance analysis, team strategy, talent scouting, and player training.</obo:IAO_0000115>
        <rdfs:label xml:lang="en">xGPhilosophy</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/FutOn_00074 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/FutOn_00074">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/FutOn_00002"/>
        <obo:IAO_0000115 xml:lang="en">A refereeAct that represents the action of making decisions on rule infractions during a game.</obo:IAO_0000115>
        <rdfs:label xml:lang="en">decisionAct</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/FutOn_00075 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/FutOn_00075">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/FutOn_00074"/>
        <obo:IAO_0000115 xml:lang="en">A decisionAct that represents the referee&apos;s action of making a decision related to fouls committed by players during a game.</obo:IAO_0000115>
        <rdfs:label xml:lang="en">foulDecisionAct</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/FutOn_00076 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/FutOn_00076">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/FutOn_00075"/>
        <obo:IAO_0000115 xml:lang="en">A foulDecisionAct that signifies the referee&apos;s action of making a decision regarding a suspected handball offense.</obo:IAO_0000115>
        <rdfs:label xml:lang="en">handballCall</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/FutOn_00077 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/FutOn_00077">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/FutOn_00075"/>
        <obo:IAO_0000115 xml:lang="en">A foulDecisionAct that signifies the referee&apos;s action of deciding on potential offside situations in a game.</obo:IAO_0000115>
        <rdfs:label xml:lang="en">offsideDecisionAct</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/FutOn_00078 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/FutOn_00078">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/FutOn_00075"/>
        <obo:IAO_0000115 xml:lang="en">A foulDecisionAct that encompasses the referee&apos;s action of deciding to issue a yellow or red card to a player.</obo:IAO_0000115>
        <rdfs:label xml:lang="en">cardDecisionAct</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/FutOn_00079 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/FutOn_00079">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/FutOn_00078"/>
        <obo:IAO_0000115 xml:lang="en">A cardDecisionAct that represents the referee&apos;s action of deciding to issue a yellow card to a player for a particular misconduct.</obo:IAO_0000115>
        <rdfs:label xml:lang="en">yellowCardDecisionAct</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/FutOn_00080 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/FutOn_00080">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/FutOn_00078"/>
        <obo:IAO_0000115 xml:lang="en">A cardDecisionAct that signifies the referee&apos;s action of deciding to issue a red card, either for a severe infraction or for a second yellow card offense in the same game.</obo:IAO_0000115>
        <rdfs:label xml:lang="en">redCardDecisionAct</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/FutOn_00081 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/FutOn_00081">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/FutOn_00002"/>
        <obo:IAO_0000115 xml:lang="en">A refereeAct that denotes the action of starting and stopping play during a game.</obo:IAO_0000115>
        <rdfs:label xml:lang="en">startingStoppingAct</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/FutOn_00082 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/FutOn_00082">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/FutOn_00081"/>
        <obo:IAO_0000115 xml:lang="en">A startingStoppingAct that signifies the referee&apos;s action of restarting the game after a foul has been addressed.</obo:IAO_0000115>
        <rdfs:label xml:lang="en">foulRestartAct</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/FutOn_00083 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/FutOn_00083">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/FutOn_00081"/>
        <obo:IAO_0000115 xml:lang="en">A startingStoppingAct that represents the referee&apos;s action of stopping the game due to a foul.</obo:IAO_0000115>
        <rdfs:label xml:lang="en">foulStopAct</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/FutOn_00084 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/FutOn_00084">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/FutOn_00081"/>
        <obo:IAO_0000115 xml:lang="en">A startingStoppingAct that represents the referee&apos;s action of initiating the start of a game.</obo:IAO_0000115>
        <rdfs:label xml:lang="en">gameStartAct</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/FutOn_00085 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/FutOn_00085">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/FutOn_00081"/>
        <obo:IAO_0000115 xml:lang="en">A startingStoppingAct that signifies the referee&apos;s action of calling the end of a game.</obo:IAO_0000115>
        <rdfs:label xml:lang="en">gameStopAct</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/FutOn_00086 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/FutOn_00086">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/FutOn_00081"/>
        <obo:IAO_0000115 xml:lang="en">A startingStoppingAct that denotes the referee&apos;s action of initiating the start of the second half of a game.</obo:IAO_0000115>
        <rdfs:label xml:lang="en">halfTimeStartAct</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/FutOn_00087 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/FutOn_00087">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/FutOn_00081"/>
        <obo:IAO_0000115 xml:lang="en">A startingStoppingAct that embodies the referee&apos;s action of declaring the end of the first half of a game.</obo:IAO_0000115>
        <rdfs:label xml:lang="en">halfTimeStopAct</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/FutOn_00088 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/FutOn_00088">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/FutOn_00002"/>
        <obo:IAO_0000115 xml:lang="en">A refereeAct that encompasses all actions related to keeping track of time during a game.</obo:IAO_0000115>
        <rdfs:label xml:lang="en">timeKeepingAct</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/FutOn_00089 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/FutOn_00089">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/FutOn_00088"/>
        <obo:IAO_0000115 xml:lang="en">A timeKeepingAct that represents the referee&apos;s action of monitoring the overall game clock.</obo:IAO_0000115>
        <rdfs:label xml:lang="en">gameClockMonitoringAct</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/FutOn_00090 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/FutOn_00090">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/FutOn_00088"/>
        <obo:IAO_0000115 xml:lang="en">A timeKeepingAct that denotes the referee&apos;s action of adding or subtracting stoppage time due to delays like injuries or interruptions.</obo:IAO_0000115>
        <rdfs:label xml:lang="en">stoppageTimeAdjustmentAct</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/FutOn_00091 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/FutOn_00091">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/FutOn_00088"/>
        <obo:IAO_0000115 xml:lang="en">A timeKeepingAct that signifies the referee&apos;s action of keeping track of the time elapsed in each half of the game.</obo:IAO_0000115>
        <rdfs:label xml:lang="en">halfTimeMonitoringAct</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/FutOn_00092 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/FutOn_00092">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/FutOn_00088"/>
        <obo:IAO_0000115 xml:lang="en">A timeKeepingAct that embodies the referee&apos;s action of keeping track of additional time periods in games that may go into extra time or overtime.</obo:IAO_0000115>
        <rdfs:label xml:lang="en">extraTimeMonitoringAct</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/FutOn_00093 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/FutOn_00093">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/FutOn_00002"/>
        <obo:IAO_0000115 xml:lang="en">A refereeAct that signifies actions aimed at enforcing the rules of the game and ensuring fair play.</obo:IAO_0000115>
        <rdfs:label xml:lang="en">fairPlayEnforcementAct</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/FutOn_00094 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/FutOn_00094">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/FutOn_00093"/>
        <obo:IAO_0000115 xml:lang="en">A fairPlayEnforcementAct that signifies the referee&apos;s action of penalizing player misconducts, in order to maintain fair play.</obo:IAO_0000115>
        <rdfs:label xml:lang="en">misconductPenalizingAct</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/FutOn_00095 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/FutOn_00095">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/FutOn_00093"/>
